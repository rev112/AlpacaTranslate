{
  "env": {
    "browser": true,
    "es6": true,
    "webextensions": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "consistent-return": "error",
    "no-console": 0,
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  }
}
