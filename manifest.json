{
  "manifest_version": 2,
  "name": "AlpacaTranslate",
  "version": "1.0",

  "description": "Select a word and show a pop up with it",

  "icons": {
    "48": "icons/beasts-48.png"
  },
  "permissions": [
    "*://linguee.com/*",
    "*://www.linguee.com/*"
  ],

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["select.js"]
    }
  ]
}
